{
    "name": "timonnorth/service-domino",
    "type": "library",
    "description": "Domino API server",
    "keywords": [
        "timonnorth",
        "service-domino"
    ],
    "homepage": "https://github.com/oitimon/domino",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Ieremeev",
            "homepage": "https://github.com/oitimon",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "ext-gettext": "*",
        "ext-json": "*",
        "datto/json-rpc": "^6.1",
        "datto/json-rpc-http": "^5.0",
        "php-di/php-di": "^6.2",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^4.0",
        "symfony/lock": "^5.1"
    },
    "require-dev": {
        "mollie/php-coding-standards": "^1.2",
        "phpunit/phpunit" : "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
